<section>
  <table class="govuk-table">
    <tbody>
    <% {
           housing: :moe_housing,
           childcare: :moe_childcare,
           maintenance_out: :moe_maintenance_out,
           legal_aid: :moe_legal_aid
       }.each do |name, value_method|
    %>
      <%= render 'shared/means_report/item', name: name, scope: 'outgoing', value_method: value_method %>
    <% end  %>
    <%= render 'shared/means_report/total', name: 'total_outgoing', scope: 'outgoing', value_method: :total_monthly_outgoings %>
    </tbody>
  </table>
</section>
